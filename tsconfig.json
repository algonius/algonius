{
  "extends": "plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/index.d.ts",
    "./**/*.ts",
    "./**/*.tsx"
  ],
  "compilerOptions": {
    "target": "ES2020",
    "module": "esnext",
    "paths": {
      "~*": [
        "./src/*"
      ]
    },
    "baseUrl": ".",
    "verbatimModuleSyntax": false,
    "jsx": "react-jsx"
  }
}